<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="br.uea.mypay.tutoriais.demomylaundry">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activities.FecharPedidoActivity">
        </activity>
        <activity android:name=".activities.ListaServicoActivity" />
        <activity android:name=".activities.InicializacaoActivity" />
        <activity android:name=".activities.FecharCaixaActivity" />
        <activity
            android:name=".activities.NovoPedidoActivity"
            android:parentActivityName=".activities.MainActivity" />
        <activity
            android:name=".activities.ResumoPedidoActivity"
            android:parentActivityName=".activities.NovoPedidoActivity" />
        <activity android:name=".activities.AlterarPrecosActivity">
        </activity>
        <activity android:name=".activities.AbrirCaixaActivity" />
        <activity android:name=".activities.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activities.CadastroManualActivity"
            android:parentActivityName=".activities.CadastroMainActivity" />
        <activity
            android:name=".activities.CadastroWebActivity"
            android:parentActivityName=".activities.CadastroMainActivity" />
        <activity android:name=".activities.CadastroMainActivity" />
    </application>

</manifest>